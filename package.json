{
  "name": "tsb-web-official",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm lint && next dev -p 8500",
    "build": "next build",
    "start": "next start",
    "lint": "pnpm biome check . && pnpm tsc",
    "fix": "pnpm biome check --fix .",
    "fix-unsafe": "pnpm biome check --fix --unsafe .",
    "format": "pnpm biome format --write .",
    "clear-all": "rm -rf node_modules .next out dist build",
    "re-start": "rm -rf node_modules .next out dist build && pnpm install && pnpm dev",
    "re-build": "rm -rf node_modules .next out dist build && pnpm install && pnpm build",
    "install-all": "pnpm ci && cd public/api && composer install",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@fullpage/react-fullpage": "^0.1.42",
    "@mui/material": "^6.0.0",
    "@tensorflow/tfjs": "^4.20.0",
    "@tensorflow/tfjs-layers": "^4.20.0",
    "bootstrap": "^5.3.3",
    "classnames": "^2.5.1",
    "firebase": "^10.13.0",
    "html-react-parser": "^5.1.13",
    "lightgallery": "^2.7.2",
    "next": "^14.2.6",
    "nodemailer": "^6.9.14",
    "react": "^18.3.1",
    "react-countup": "^6.5.3",
    "react-dom": "^18.3.1",
    "react-google-recaptcha": "^3.1.0",
    "react-icons": "^5.3.0",
    "react-number-format": "^5.4.1",
    "react-scrollmagic": "^2.3.0",
    "react-select": "^5.8.0",
    "react-simple-animate": "^3.5.2",
    "react-sketch-canvas": "^6.2.0",
    "react-textarea-autosize": "^8.5.3",
    "react-toastify": "^10.0.5",
    "reactstrap": "^9.2.2",
    "recharts": "^2.12.7",
    "sass": "^1.77.8",
    "styled-components": "^6.1.12",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@types/gtag.js": "^0.0.20",
    "@types/react": "18.3.4",
    "@types/react-google-recaptcha": "^2.1.9",
    "@types/styled-components": "^5.1.34",
    "@types/uuid": "^10.0.0",
    "eslint": "9.9.1",
    "tailwindcss": "^3.4.10",
    "typescript": "5.5.4"
  }
}
